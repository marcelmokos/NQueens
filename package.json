    "jest": "^18.1.0",
    "jest-cli": "^18.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-async-await": "^0.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^1.0.2",
